{
    "name": "autoves",
    "desc": "Automatically compiles vesti file",
    "args": [
        {
            "meta": "FILE",
            "desc": "file to compile",
            "type": "string",
            "default": null
        }
    ],
    "flags": [
        {
            "long": "latex",
            "short": "L",
            "desc": "compile with latex",
            "type": "bool",
            "default": "false"
        },
        {
            "long": "pdf",
            "short": "p",
            "desc": "compile with pdflatex",
            "type": "bool",
            "default": "false"
        },
        {
            "long": "xe",
            "short": "x",
            "desc": "compile with xelatex",
            "type": "bool",
            "default": "false"
        },
        {
            "long": "lua",
            "short": "l",
            "desc": "compile with lualatex",
            "type": "bool",
            "default": "false"
        }
    ],
    "subcmds": []
}
